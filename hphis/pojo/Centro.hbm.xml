<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hphis.pojo.Centro" table="MY_CENTRO">

        <id name="codCentro" type="java.lang.Integer" column="COD_CENTRO">
            <generator class="sequence">
                <param name="sequence">S_CENTRO</param>
            </generator>
        </id>

        <property
            name="centro"
            type="com.hphis.corp.hibernate.types.NullStringType"
            column="NOMBRE_CENTRO"
            not-null="true"
            length="200"/>

    </class>

</hibernate-mapping>